{
  "name": "scraper.js-queue-bull",
  "version": "3.0.2",
  "description": "A queue plugin for Scraper.js that is backed by Bull",
  "main": "dist/queue-bull.js",
  "scripts": {
    "lint": "eslint ./src",
    "unit": "NODE_ENV=test mocha --reporter spec --compilers js:babel-core/register",
    "test": "npm run lint && nyc --reporter lcov npm run unit && codeclimate-test-reporter < ./coverage/lcov.info",
    "prepublish": "rm -rf ./dist && babel -d ./dist ./src"
  },
  "repository": "leahcimic/scraperjs-queue-bull",
  "author": "Michael Leaney <leahcimic@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.6",
    "bull": "leahciMic/bull#Add-support-for-rate-limiting",
    "redis": "^2.6.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-generator": "^6.19.0",
    "babel-helpers": "^6.16.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.4.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.1.2",
    "nyc": "^9.0.1",
    "redis-mock": "^0.16.0"
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
