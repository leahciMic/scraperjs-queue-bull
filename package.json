{
  "name": "scraper.js-queue-bull",
  "version": "2.0.0",
  "description": "A queue plugin for Scraper.js that is backed by Bull",
  "main": "dist/queue-bull.js",
  "scripts": {
    "lint": "eslint ./src",
    "unit": "NODE_ENV=test mocha --reporter spec --compilers js:babel-core/register",
    "test": "npm run lint && nyc --reporter lcov npm run unit && codeclimate-test-reporter < ./coverage/lcov.info",
    "prepublish": "rm -rf ./dist && babel -d ./dist ./src"
  },
  "repository": "leahcimic/scraperjs-queue-bull",
  "author": "Michael Leaney <leahcimic@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.1",
    "bull": "^1.0.0",
    "redis": "^2.6.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-generator": "^6.11.4",
    "babel-helpers": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-3": "^6.11.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.3",
    "eslint": "^3.3.0",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.13.0",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0",
    "redis-mock": "^0.10.0"
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
